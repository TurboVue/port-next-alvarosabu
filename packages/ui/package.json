{
  "name": "@alvarosabu/ui",
  "version": "2.8.1",
  "description": "UI Library for AS development",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/as-ui.umd.js",
  "module": "./dist/as-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/as-ui.es.js",
      "require": "./dist/as-ui.umd.js"
    },
    "./styles": "./dist/style.css",
    "./theme": "./styles/as-theme.css",
    "./": "./dist/as-ui.es.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "histoire": "pnpm exec histoire dev",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:components": "cypress open-ct"
  },
  "dependencies": {
    "@vueuse/core": "^8.4.2",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.3",
    "@cypress/vue": "^3.1.2",
    "@iconify-json/ant-design": "^1.1.1",
    "@iconify-json/carbon": "^1.1.4",
    "@iconify-json/gg": "^1.1.1",
    "@iconify-json/logos": "^1.1.5",
    "@iconify-json/octicon": "^1.1.8",
    "@iconify-json/teenyicons": "^1.1.1",
    "@iconify/json": "^2.1.43",
    "@types/node": "^17.0.35",
    "@unocss/preset-attributify": "^0.33.4",
    "@unocss/preset-icons": "^0.33.4",
    "@unocss/preset-typography": "^0.37.4",
    "@unocss/preset-uno": "^0.33.4",
    "@unocss/preset-web-fonts": "^0.33.4",
    "@unocss/reset": "^0.33.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0-rc.21",
    "c8": "^7.11.2",
    "cypress": "^9.6.1",
    "histoire": "^0.4.6",
    "jsdom": "^19.0.0",
    "pathe": "^0.3.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.51.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "unocss": "^0.33.4",
    "vite": "^2.9.9",
    "vite-plugin-banner": "^0.2.0",
    "vite-plugin-dts": "^1.1.1",
    "vite-svg-loader": "^3.3.0",
    "vitest": "^0.10.5",
    "vue-tsc": "^0.34.13"
  }
