{
  "name": "@alvarosabu/ui",
  "version": "2.9.1",
  "description": "UI Library for AS development",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/as-ui.umd.js",
  "module": "./dist/as-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/as-ui.es.js",
      "require": "./dist/as-ui.umd.js"
    },
    "./styles": "./dist/style.css",
    "./theme": "./styles/as-theme.css",
    "./": "./dist/as-ui.es.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
