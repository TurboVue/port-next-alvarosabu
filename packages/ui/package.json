    "coverage": "vitest run --coverage",
    "cy": "cypress open"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.1",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.0.0",
    "@cypress/vue": "^4.0.0",
    "@iconify-json/ant-design": "^1.1.3",
    "@iconify-json/carbon": "^1.1.6",
    "@iconify-json/gg": "^1.1.2",
    "@iconify-json/logos": "^1.1.8",
    "@iconify-json/octicon": "^1.1.12",
    "@iconify-json/teenyicons": "^1.1.3",
    "@iconify/json": "^2.1.75",
    "@types/node": "^18.0.3",
    "@types/prismjs": "^1.26.0",
    "@unocss/preset-attributify": "^0.44.0",
