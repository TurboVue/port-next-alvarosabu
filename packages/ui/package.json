    "@unocss/reset": "^0.44.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0-rc.21",
    "c8": "^7.11.2",
    "cypress": "^10.3.0",
    "histoire": "^0.7.9",
    "jsdom": "^20.0.0",
    "pathe": "^0.3.2",
    "prism-theme-vars": "^0.2.3",
    "prismjs": "^1.28.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.53.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unocss": "^0.44.0",
    "vite": "^2.9.14",
    "vite-plugin-banner": "^0.3.0",
